local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- üîê Ton vrai lien LootLabs (√† ne pas modifier)
local lootlabsLink = "https://lootlabs.net/key-system/5e0d4ff2259d74a36ffccfeafe4c0e414d67fa5965b8e47f923a40fa6e5262a9"

-- üéØ Copier le lien dans le presse-papiers
GetKey.MouseButton1Click:Connect(function()
	setclipboard(lootlabsLink)
	print("Lien LootLabs copi√© :", lootlabsLink)
end)

-- ‚úÖ V√©rification de la cl√©
CheckKey.MouseButton1Click:Connect(function()
	local key = textBox.Text -- la cl√© entr√©e par l'utilisateur
	local response
	local success, result = pcall(function()
		response = game:HttpGet("https://yourserver.com/keys/"..key..".txt")
	end)

	if success and response then
		loadstring(response)()
	else
		warn("Cl√© invalide ou expir√©e.")
	end
end)
